package helloWorldusingJava8;

public class Student {                // Defining Student POJO class 

	private String sid;                // Declaring a private data memeber eid
	private String sname;              // Declaring a private data memeber ename
	 
	public Student(){                  // Declaring public default constructor
		super();
	}
	public Student(String sid, String sname) {     // Declaring public constructor and passing eid, ename
		super();
		this.sid = sid;
		this.sname = sname;
	}
	public String getSid() {       // declaring getter method for eid
		return sid;
	}
	public void setEid(String sid) {  // declaring setter method for eid
		this.sid = sid;
	}
	public String getSname() {     // declaring getter method for ename
		return sname;
	}
	public void setSname(String sname) {  // declaring setter method for ename
		this.sname = sname;
	}
	
	@Override
	public String toString() {      // returning the values using toString method.
		return "Student [eid=" + sid + ", sname=" + sname + "]";
	}
	
}
